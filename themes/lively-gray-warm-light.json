        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#ECB3B3"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#FFEFBF",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#FFEFBF",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "entity.name.class",
            "settings": {
                "foreground": "#F0D070"
            }
        },
        {
            "scope": "entity.name.function",
